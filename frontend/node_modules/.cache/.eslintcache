item.qty} \n                        onChangeCapture={(e) => dispatch(addToCart(item.product, Number(e.target.value)))}\n                        >\n                        \n                          {\n                        \n                          \n                          [...Array(item.countInStock).keys()].map( (x) => (\n                            <option key={x + 1} value={ x + 1}>\n                              {x+1}\n                            </option>\n                          ))}\n                        \n                        </Form.Control>\n\n                  </Col>\n                  <Col md={2}>\n                    <Button type='button' variant='light' onClick={() => removeFromCartHandler(item.product)}>\n                      <i className='fas fa-trash'></i>\n                    </Button>\n                  </Col>\n              </Row>\n            </ListGroup.Item>\n          ))}\n\n        </ListGroup>\n      )}\n\n    </Col> \n    <Col md = {2}>\n\n    </Col>\n\n    <Col md = {2}>\n\n    </Col>\n\n  </Row>\n}\n\nexport default CartScreen\n","/Users/anair/project1/frontend/src/reducers/cartReducers.js",[],"/Users/anair/project1/frontend/src/constants/cartConstants.js",[],"/Users/anair/project1/frontend/src/actions/cartActions.js",[],{"ruleId":"87","severity":1,"message":"88","line":3,"column":21,"nodeType":"89","messageId":"90","endLine":3,"endColumn":30},{"ruleId":"87","severity":1,"message":"91","line":1,"column":16,"nodeType":"89","messageId":"90","endLine":1,"endColumn":24},{"ruleId":"87","severity":1,"message":"92","line":7,"column":21,"nodeType":"89","messageId":"90","endLine":7,"endColumn":29},{"ruleId":"87","severity":1,"message":"93","line":19,"column":9,"nodeType":"89","messageId":"90","endLine":19,"endColumn":18},{"ruleId":"94","severity":1,"message":"95","line":34,"column":5,"nodeType":"96","endLine":34,"endColumn":15,"suggestions":"97"},{"ruleId":"87","severity":1,"message":"98","line":4,"column":51,"nodeType":"89","mess